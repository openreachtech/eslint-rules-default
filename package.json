{
  "name": "@openreachtech/eslint-rules-default",
  "version": "1.1.1",
  "description": "A showcasing package all the rules of ESLint.",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "node": "./index.js",
      "default": "./index.js"
    }
  },
  "files": [
    "index.js",
    "index.mjs",
    "README.ja.md",
    "configurations/",
    "types/"
  ],
  "scripts": {
    "l": "npm run lint",
    "lint": "eslint .",
    "test": "export NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\"; jest --forceExit --maxWorkers=5"
  },
  "type": "commonjs",
  "keywords": [
    "ESLint"
  ],
  "author": "Open Reach Tech Inc.",
  "license": "MIT",
  "dependencies": {
    "@eslint/js": "^9.31.0",
    "@stylistic/eslint-plugin": "^5.2.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsdoc": "^51.4.1",
    "eslint-plugin-openreachtech": "^1.0.1",
    "jest": "^30.0.4"
  }
}
